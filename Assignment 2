//Grass
stroke(#399F33);
strokeWeight(250);
noFill();
bezier(0,489,185,459,660,559,800,520);

//Balloons

fill(200,25,10);
noStroke();
beginShape();
vertex(235, 250);
vertex(278,202);
vertex(235, 159);
vertex(192, 202);
endShape(CLOSE);
noStroke();
arc(235,160,120,120,HALF_PI+QUARTER_PI, HALF_PI+HALF_PI+PI+QUARTER_PI);

stroke(255);
strokeWeight(1);
noFill();
bezier(235,250,250,260,245,270,290,300);


fill(160,50,200);
noStroke();
beginShape();
vertex(700, 72);
vertex(689, 60);
vertex(700, 49);
vertex(711, 60);
endShape(CLOSE);
noStroke();
arc(700,50,30,30,HALF_PI+QUARTER_PI, HALF_PI+HALF_PI+PI+QUARTER_PI);

stroke(255);
strokeWeight(1);
noFill();
bezier(700,72,702,74,700,78,707,79);


fill(200,100,180);
noStroke();
beginShape();
vertex(640, 360);
vertex(610,327);
vertex(640, 299);
vertex(669, 328);
endShape(CLOSE);
noStroke();
arc(640,300,80,80,HALF_PI+QUARTER_PI, HALF_PI+HALF_PI+PI+QUARTER_PI);


stroke(255);
strokeWeight(1);
noFill();
bezier(640,360,647,364,660,361,670,374);

colorMode(RGB);
fill(255,255,255);
strokeWeight(10);
noFill();
arc(235,140,100,60,HALF_PI+HALF_PI, HALF_PI+HALF_PI+QUARTER_PI);

