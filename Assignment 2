void setup() {
  size(800,600);
}

void draw(){
  background(126,192,255);
//House
stroke(0,0,0);
strokeWeight(1);
fill(#A56511);
quad(10,300, 10, 500,80, 500,80, 300 );

fill(#A5080B);
quad(60,330, 60, 500,75, 500,75, 330 );

strokeWeight(2);
stroke(#EDDA05);
point(72, 345);

